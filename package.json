{
  "name": "quaint-highlight",
  "description": "Syntax highlighting for Quaint using highlight.js",
  "version": "0.0.6",
  "format": "cjs",
  "repository": {
    "type": "git",
    "url": "https://github.com/breuleux/quaint-highlight"
  },
  "keywords": [
    "quaint",
    "highlight",
    "highlighting"
  ],
  "author": "Olivier Breuleux",
  "license": "MIT",
  "main": "./lib/index.js",
  "dependencies": {
    "chalk": "^1.1.1",
    "earlgrey-runtime": ">=0.0.11",
    "highlight.js": "^8.7.0",
    "inquirer-promise": "0.0.3",
    "quaint": ">=0.0.8"
  },
  "devDependencies": {
    "earlgrey": ">=0.0.12",
    "mocha": "^2.2.1",
    "earl-mocha": ">=0.0.2"
  },
  "scripts": {
    "refresh": "earl compile -5vso lib/ src/",
    "compile": "earl compile -5rvso lib/ src/",
    "prepublish": "npm run compile",
    "test": "mocha"
  }
}
